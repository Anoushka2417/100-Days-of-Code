class Solution
{
public:
	public:
		int find_median(vector<int> v)
		{
		    // Code here.
		    int ans;
		    int size=v.size();
		    sort(v.begin(),v.end());
		    
		    if (size%2==0){
		        ans=(v[size/2-1]+v[size/2])/2;
		    }
		    else{
		        ans =v[size/2];
		    }
		    return ans;
		}
};
